# OpenClaw Knowledgebase Configuration

# Supabase (required)
SUPABASE_URL=http://localhost:8000
SUPABASE_KEY=your-service-key-here
TABLE_PREFIX=kb  # Table prefix: kb_sources, kb_chunks

# Ollama (optional, defaults shown)
OLLAMA_URL=http://localhost:11434
EMBEDDING_MODEL=nomic-embed-text
EMBEDDING_DIMENSIONS=768

# Chunking (optional)
CHUNK_SIZE=1000
CHUNK_OVERLAP=200

# Search (optional)
DEFAULT_MATCH_COUNT=10
SIMILARITY_THRESHOLD=0.5
SEMANTIC_WEIGHT=0.7

# OpenClaw Memory Module (optional â€” leave empty for legacy single-tenant mode)
OPENCLAW_AGENT_NAME=
OPENCLAW_AGENT_KEY=
